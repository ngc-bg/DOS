@ECHO OFF
SET TEMP = C:\Temp
SET TMP  = C:\Temp
SET PATH=c:\dos;c:\nc;c:\tools;c:\tools\dosutils;c:\tools\unix;c:\4dos;c:\tools\me
goto %config%

:Q4DWS
C:\QEMM\LOADHI /R:2 /res=1296 /sqf C:\4DOS\KSTACK.COM
C:\QEMM\LOADHI /R:1 C:\TOOLS\CYR.COM /Q /M
C:\QEMM\LOADHI /R:1 C:\DOS\SHSUCDX\SHSUCDX.COM /QQ /D:SHSU001
C:\QEMM\LOADHI /R:3 /lo C:\DOS\CTMOUSE.EXE
C:\QEMM\LOADHI /R:1 /lo C:\DOSIDLE\DOSIDLE.EXE -Fm1 -Cpu

alias /R c:\4dos\aliases
auto
GOTO END

:M4DWS
LH C:\4DOS\KSTACK.COM
LH C:\TOOLS\CYR.COM /Q /M
LH /L:2 C:\DOS\SHSUCDX\SHSUCDX.COM /QQ /D:SHSU001
LH C:\DOS\CTMOUSE.EXE
rem LH C:\DOSIDLE\DOSIDLE.EXE

alias /R c:\4dos\aliases
auto

GOTO END

:MDWS
LH C:\TOOLS\CYR.COM /Q /M
LH /L:2 C:\DOS\SHSUCDX\SHSUCDX.COM /QQ /D:SHSU001
LH C:\DOS\CTMOUSE.EXE
LH C:\DOS\DOSKEY.COM

GOTO END

:XMC
LH C:\DOS\CTMOUSE.EXE
LH C:\DOS\DOSKEY.COM
LH /L:1 C:\DOS\SHSUCDX\SHSUCDX.COM /QQ /D:SHSU001

GOTO END

:CMC
LH C:\DOS\CTMOUSE.EXE
LH C:\DOS\DOSKEY.COM
LH C:\DOS\SHSUCDX\SHSUCDX.COM /QQ /D:SHSU001

GOTO END

rem :WIN
rem LH C:\DOS\CTMOUSE.EXE
rem LH C:\DOS\DOSKEY.COM
rem LH C:\DOS\SHSUCDX\SHSUCDX.COM /QQ /D:SHSU001
rem LH C:\WINDOWS\SMARTDRV.EXE /L
rem LH C:\WINDOWS\NET START

rem GOTO END

:END
